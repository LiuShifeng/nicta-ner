  -- other-modules:
  -- other-extensions:
  build-depends:       base >=4.6 && <4.7
                       , aeson >=0.7 && <0.8
                       , bytestring >=0.10 && <0.11
                       , conduit >=1.1 && <1.2
                       , conduit-extra >=1.1 && <1.2
                       , containers >=0.5 && <0.6
                       , http-conduit >=2.1 && <2.2
                       , text >=1.1 && <1.2
                       , transformers >=0.4 && <0.5
